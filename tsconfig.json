{
	"compilerOptions": {
		"outDir": "./build/",
		"sourceMap": true,
		"noImplicitAny": false,
		"module": "es2015",
		"target": "es2017",
		"jsx": "react",
		"typeRoots": [
			"node_modules/@types"
		],
		"lib": ["es2017", "dom"],
		"allowJs": true,
		"moduleResolution": "node"
	},
	"include": [
		"./src/*"
	]
}
